page {
  background-color: #fff;
}
.container .noBooks {
  text-align: center;
}
.container .bookrackContainer .titleview {
  display: flex;
  height: 60rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  margin-left: 16px;
  margin-right: 16px;
  border-bottom: 1px solid #f5f1f1;
}
.container .bookrackContainer .titleview .titleTag {
  flex: 1;
  font-size: 20px;
  line-height: 60rpx;
  border-bottom: 2px solid #d5a3c9;
}
.container .bookrackContainer .titleview .titleTag image {
  width: 60rpx;
  height: 60rpx;
}
.container .bookrackContainer .titleview .go {
  line-height: 60rpx;
  flex: 3;
  text-align: right;
  color: #d96ae0;
  font-weight: 500;
}
.container .bookrackContainer van-cell-group van-cell .iconview {
  margin-right: 10rpx;
}
.container .bookrackContainer van-cell-group van-cell .iconview .icon {
  border-radius: 10rpx;
  width: 120rpx;
  height: 170rpx;
}
.container .bookrackContainer van-cell-group van-cell .labelview {
  color: #5f5b5b;
  font-size: 12px;
}
.container .bookrackContainer van-cell-group van-cell .labelview .author .authorText {
  margin-bottom: 20rpx;
  width: auto;
  background-color: #d5a3c9;
  border-radius: 5px;
}
